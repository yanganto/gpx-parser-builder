  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "babel-cli": "~6.26",
    "babel-preset-env": "~1.6",
    "fs": "0.0.1-security",
    "should": "^13.2.1"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-latest": "^6.24.1",
